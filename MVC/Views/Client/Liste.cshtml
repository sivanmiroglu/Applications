@using MVC.Models.ORM;

@model List<Client>;

    <table>

        <tr>
            <td><h3>UserName</h3></td>
            <td><h3>City</h3></td>
            <td><h3>Country</h3></td>
        </tr>
        
@foreach (var item in Model)
{

      <tr>
          <td><h2>@item.UserName</h2></td>
          <td><h2> @item.City</h2></td>
          <td><h2>@item.Country</h2></td>
      </tr>

    

  
}
    </table>
